
defn cplx (r, i) :
   object :
      var real = r
      var imag = i
      method add (c) :
         defn hello2 (i) :
            printf("Hello123 ~ ", i)
         cplx(this.real + c.real,
              this.imag + c.imag)
      method sub (c) :
         cplx(this.real - c.real,
              this.imag - c.imag)
      method mul (c) :        
         cplx(this.real * c.real - this.imag * c.imag,
              this.real * c.imag + this.imag * c.real)
      method div (c) :
         var d = c.real * c.real + c.imag * c.imag
         cplx((this.real * c.real + this.imag * c.imag) / d,
              (this.imag * c.real - this.real * c.imag) / d)
      method print () :
         if this.imag < 0 :
            printf("~ - ~i", this.real, 0 - this.imag)
         else if this.imag == 0 :
            printf("~", this.real)
         else :
            printf("~ + ~i", this.real, this.imag)

defn hello (i) :
   printf("Hello ~ ", i)
   
defn world (j, k) :
   printf("World ~ ~\n", j, k)

defn main () :
   hello(3)
   world(4, 5)

main()